<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE shader SYSTEM 'DEOpenGLShader.dtd'>
<shader>
	<name>DefRen Write Skinned VBO</name>
	
	<version>140</version>
	
	<texture name='texPositions' target='0'/>
	<texture name='texNormalTangent' target='1'/>
	
	<parameter name='pTexNorTanWidth'/> <!-- 0 -->
	<parameter name='pIndexOffset'/> <!-- 1 -->
	
	<attribute name='inPositionIndex' target='0'/>
	<attribute name='inNormalIndex' target='1'/>
	<attribute name='inTangentIndex' target='2'/>
	<attribute name='inNegateTangent' target='3'/>
	
	<output name='outPosition' target='0'/>
	<output name='outRealNormal' target='1'/>
	<output name='outNormal' target='2'/>
	<output name='outTangent' target='3'/>
	
	<feedback name='outPosition'/>
	<feedback name='outRealNormal'/>
	<feedback name='outNormal'/>
	<feedback name='outTangent'/>
	
	<sourceCode unit='vertex'>v130/vertex/defren/geometry/write_skinned_vbo.glsl</sourceCode>
</shader>
