			<idGroup>spawn-point</idGroup>
			<idUsage>true</idUsage>
		</property>
		<partialHideTag>Editing</partialHideTag>
		<partialHideTag>Spawn</partialHideTag>
		<category>Dragengine</category>
	</class>
