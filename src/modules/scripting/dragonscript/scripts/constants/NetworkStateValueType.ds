namespace Dragengine.Networking


/**
 * \brief NetworkState value type.
 */
enum NetworkStateValueType
	/** \brief Integer value. */
	integer
	
	/** \brief Floating point value. */
	floating
	
	/** \brief String value. */
	string
	
	/** \brief Data value. */
	data
	
	/** \brief 2-component integer point value. */
	point2
	
	/** \brief 3-component integer point value. */
	point3
	
	/** \brief 3-component floating point vector value. */
	vector2
	
	/** \brief 3-component floating point vector value. */
	vector3
	
	/** \brief 4-ocmponent floating point quaternion value. */
	quaternion
end
