<?xml version='1.0' encoding='UTF-8'?>
<guiTheme>
	<!--
	WindowSettings:
	- "Window.Settings": Settings window
	- "Panel.Settings.Sheets": Panel containing all settings panels
	- "Panel.Settings.Buttons.Top": Top buttons panel
	- "Panel.Settings.Buttons.Bottom": Bottom buttons panel
	- "Panel.Settings.Buttons.Left": Left buttons panel
	- "Panel.Settings.Buttons.Right": Right buttons panel
	- "Panel.Settings.Buttons.Dialog": Dialog buttons panel at the bottom
	- "Button.Settings.Sheet": Button to activate a sheet
	- "Button.Settings.Save": Button to save and close window
	- "Button.Settings.Reset": Button to reset current sheet
	- "Button.Settings.Cancel": Button to discard changes and close window
	-->
	
	<designer name='Window.Settings' extendNamed='Window'>
		<borderSize parameter='normal.padding' all='20'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Top' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='0' right='0' bottom='10'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Bottom' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='10' right='0' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Left' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='0' right='10' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Right' type='Widget'>
		<borderSize parameter='normal.padding' left='10' top='0' right='0' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Dialog' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='20' right='0' bottom='0'/>
	</designer>
</guiTheme>
