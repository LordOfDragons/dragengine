<?xml version='1.0' encoding='UTF-8'?>
<guiTheme>
	<!--
		WindowConsole
	-->
	<designer name='WindowConsole' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.8'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	<designer name='TextField.ConsoleInput' type='TextField'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	
	
	<!--
		ShoutConsole
	-->
	<designer name='ShoutConsole' type='Widget'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='31'>../modern/fonts/subtitle.defont</font>
	</designer>
</guiTheme>
