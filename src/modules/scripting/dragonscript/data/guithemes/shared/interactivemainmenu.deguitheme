<?xml version='1.0' encoding='UTF-8'?>
<guiTheme>
	<!--
	Interactive main menu. These designers can be define:
	- 'InteractiveMainMenu' -> InteractiveMainMenu
	-->
	
	<designer name='InteractiveMainMenu' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
	</designer>
	
	<designer name='Panel.IMMTextDisplay' type='Widget'>
		<borderSize parameter='normal.padding' all='50'/>
	</designer>
	
	<designer name='Label.IMMTextDisplay' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='30'>../modern/fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='20' top='3' right='20' bottom='3'/>
	</designer>
</guiTheme>
