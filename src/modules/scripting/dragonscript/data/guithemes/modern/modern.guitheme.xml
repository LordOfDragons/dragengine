<?xml version='1.0' encoding='ISO-8859-1'?>
<guiTheme>
	<!-- Default widget parameters. -->
	<font for='widget' size='15'>fonts/sans15.defont</font>
	
	<!-- Widget designers -->
	<designer name='Desktop' type='Desktop'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Image'>
			<image parameter='image'>mousepointer/default.png</image>
		</mousePointer>
	</designer>
	
	<designer name='Window' type='Window'>
		<decoration parameter='decoration' type='CanvasCreator'>
			<borderSize parameter='decorationSize' left='8' top='25' right='8' bottom='14'/>
			<font parameter='active.title.font' size='15'>fonts/sans15.defont</font>
			<color parameter='active.title.textColor' r='1' g='1' b='1'/>
			<color parameter='inactive.title.textColor' r='0.75' g='0.75' b='0.75'/>
			<canvasCreator parameter='active.canvas' extend='window/normal.ccreator.xml'/>
			<canvasCreator parameter='inactive.canvas' extend='window/inactive.ccreator.xml'/>
			<image parameter='imageButtonCloseNormal'>window/button/close_normal.png</image>
			<image parameter='imageButtonCloseHover'>window/button/close_hover.png</image>
			<image parameter='imageButtonClosePressed'>window/button/close_pressed.png</image>
			<image parameter='imageButtonMaximizeNormal'>window/button/maximize_normal.png</image>
			<image parameter='imageButtonMaximizeHover'>window/button/maximize_hover.png</image>
			<image parameter='imageButtonMaximizePressed'>window/button/maximize_pressed.png</image>
			<image parameter='imageButtonMinimizeNormal'>window/button/minimize_normal.png</image>
			<image parameter='imageButtonMinimizeHover'>window/button/minimize_hover.png</image>
			<image parameter='imageButtonMinimizePressed'>window/button/minimize_pressed.png</image>
		</decoration>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.85' g='0.82' b='0.82'/>
		</canvasCreator>
	</designer>
	
	<designer name='WindowPopup' type='Window'>
		<!--<decoration parameter='decoration' type='CanvasCreator'>
			<borderSize parameter='decorationSize' all='1'/>
			<canvasCreator parameter='active.canvas' type='Bevel'>
				<color parameter='colorHighlight' r='1' g='1' b='1'/>
				<color parameter='colorShadow' r='0.6' g='0.6' b='0.6'/>
			</canvasCreator>
		</decoration>-->
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.85' g='0.82' b='0.82'/>
		</canvasCreator>
	</designer>
	
	<designer name='WindowToolTip' type='Window'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'>
			<colorMatrix parameter='colorMatrix'>
				<scale r='0' g='0' b='0' a='0.5'/>
			</colorMatrix>
		</canvasCreator>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'>
				<colorMatrix parameter='colorMatrix'>
					<scale r='0.75' g='0.75' b='0.75' a='0.65'/>
				</colorMatrix>
			</canvasCreator>
			<borderSize parameter='size' all='2'/>
		</border>
		<borderSize parameter='normal.padding' all='2'/>
	</designer>
	
	<designer name='Label.WindowToolTip' type='Label'>
		<string parameter='normal.alignment'>left,top</string>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
	</designer>
	
	<designer name='WindowFullScreen' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
	</designer>
	
	<designer name='Window.Dialog' extendNamed='Window'>
		<borderSize parameter='normal.padding' all='20'/>
	</designer>
	
	<designer name='Panel.WindowDialogButtons' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='20' right='0' bottom='0'/>
	</designer>
	
	<designer name='Label' type='Label'>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
		<color parameter='disabled.textColor' r='0.5' g='0.5' b='0.5'/>
	</designer>
	
	<designer name='Image' type='Image'>
		<colorMatrix parameter='normal.imageColorTransform'/>
		<colorMatrix parameter='disabled.imageColorTransform'>
			<saturation r='0' g='0' b='0'/>
			<contrast r='0.5' g='0.5' b='0.5'/>
			<brightness r='0.15' g='0.15' b='0.15' a='0'/>
		</colorMatrix>
	</designer>
	
	<designer name='Button' type='Button'>
		<canvasCreator parameter='normal.canvas' extend='button/normal.ccreator.xml'/>
		<canvasCreator parameter='pressed.canvas' extend='button/pressed.ccreator.xml'/>
		<canvasCreator parameter='disabled.canvas' extend='button/disabled.ccreator.xml'/>
		<canvasCreator parameter='hover.canvas' extend='button/hover.ccreator.xml'/>
		<borderSize parameter='normal.padding' all='5'/>
		<borderSize parameter='pressed.padding' left='6' top='6' right='4' bottom='4'/>
	</designer>
	
	<designer name='ScrollBar' type='ScrollBar'>
		<canvasCreator parameter='canvasFrame' extend='scrollbar/frame.ccreator.xml'/>
		<canvasCreator parameter='canvasKnob' extend='scrollbar/knob.ccreator.xml'/>
		<canvasCreator parameter='canvasKnobHover' extend='scrollbar/knob-hover.ccreator.xml'/>
		
		<integer parameter='size'>16</integer>
		
		<designer parameter='buttonUp' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonDown' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonLeft' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonRight' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		
		<image parameter='imageUpNormal'>arrows/up_normal.png</image>
		<image parameter='imageUpHover'>arrows/up_hover.png</image>
		<image parameter='imageDownNormal'>arrows/down_normal.png</image>
		<image parameter='imageDownHover'>arrows/down_hover.png</image>
		<image parameter='imageLeftNormal'>arrows/left_normal.png</image>
		<image parameter='imageLeftHover'>arrows/left_hover.png</image>
		<image parameter='imageRightNormal'>arrows/right_normal.png</image>
		<image parameter='imageRightHover'>arrows/right_hover.png</image>
	</designer>
	
	<designer name='SpinButton' type='SpinButton'>
		<integer parameter='size'>16</integer>
		
		<designer parameter='buttonUp' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonDown' type='Button'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		
		<image parameter='imageUpNormal'>arrows/up_normal.png</image>
		<image parameter='imageUpHover'>arrows/up_hover.png</image>
		<image parameter='imageDownNormal'>arrows/down_normal.png</image>
		<image parameter='imageDownHover'>arrows/down_hover.png</image>
	</designer>
	
	<designer name='TextField' type='TextField'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		<borderSize parameter='normal.padding' all='2'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
		
		<canvasCreator parameter='disabled.canvas' extend='framed/background-disabled.ccreator.xml'/>
		<border parameter='disabled.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/disabled.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<canvasCreator parameter='focused.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='focused.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/focused.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<border parameter='hover.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/hover.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
	</designer>
	
	<designer name='TextArea' type='TextArea'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		<borderSize parameter='normal.padding' all='2'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
		
		<canvasCreator parameter='disabled.canvas' extend='framed/background-disabled.ccreator.xml'/>
		<border parameter='disabled.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/disabled.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<canvasCreator parameter='focused.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='focused.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/focused.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<border parameter='hover.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/hover.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
	</designer>
	
	<designer name='ListBox' type='ListBox'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<canvasCreator parameter='disabled.canvas' extend='framed/background-disabled.ccreator.xml'/>
		<border parameter='disabled.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/disabled.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<canvasCreator parameter='focused.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='focused.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/focused.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		
		<border parameter='hover.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/hover.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
	</designer>
	
	<designer name='Label.ListRenderer' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Default'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5' a='0.1'/>
		</canvasCreator>
	</designer>
	
	<designer name='Label.ListRenderer.Selected' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.3' g='0.3' b='0.5'/>
		</canvasCreator>
	</designer>
	
	<designer name='ComboBox' type='ComboBox'>
		<canvasCreator parameter='normal.canvas' extend='button/normal.ccreator.xml'/>
		<canvasCreator parameter='droppedDown.canvas' extend='button/pressed.ccreator.xml'/>
		<canvasCreator parameter='disabled.canvas' extend='button/disabled.ccreator.xml'/>
		<canvasCreator parameter='hover.canvas' extend='button/hover.ccreator.xml'/>
		<borderSize parameter='normal.padding' left='4'/>
		<image parameter='imageDropDownNormal'>arrows/down_normal.png</image>
		<image parameter='imageDropDownHover'>arrows/down_hover.png</image>
	</designer>
	
	<designer name='ListBox.ComboBox' extendNamed='ListBox'>
	</designer>
	
	<designer name='TextField.ComboBoxEditor' type='TextField'>
		<!--<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='1' g='1' b='1'/>
		</canvasCreator>-->
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
	</designer>
	
	<designer name='CheckBox' type='CheckBox'>
		<borderSize parameter='normal.padding' all='5'/>
		<image parameter='normal.imageBox'>checkbox/checkbox_normal.png</image>
		<image parameter='normal.imageCheck'>checkbox/checked_normal.png</image>
		<image parameter='focused.imageBox'>checkbox/checkbox_focused.png</image>
		<image parameter='hover.imageBox'>checkbox/checkbox_hover.png</image>
		<image parameter='disabled.imageCheck'>checkbox/checked_disabled.png</image>
	</designer>
	
	<designer name='Label.CheckBox' extendNamed='Label'>
		<string parameter='normal.alignment'>left,middle</string>
	</designer>
	
	<designer name='OptionBox' type='OptionBox'>
		<borderSize parameter='normal.padding' all='5'/>
		<image parameter='normal.imageBox'>optionbox/optionbox_normal.png</image>
		<image parameter='normal.imageSelect'>optionbox/selected_normal.png</image>
		<image parameter='focused.imageBox'>optionbox/optionbox_focused.png</image>
		<image parameter='hover.imageBox'>optionbox/optionbox_hover.png</image>
		<image parameter='disabled.imageSelect'>optionbox/selected_disabled.png</image>
	</designer>
	
	<designer name='Label.OptionBox' extendNamed='Label'>
		<string parameter='normal.alignment'>left,middle</string>
	</designer>
	
	<designer name='ProgressBar' type='ProgressBar'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
		<borderSize parameter='normal.padding' left='8' top='4' right='8' bottom='4'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
		
		<canvasCreator parameter='bar' extend='framed/background.ccreator.xml'>
			<colorMatrix parameter='colorMatrix'>
				<scale r='0' g='0.5' b='1' a='1'/>
			</colorMatrix>
		</canvasCreator>
	</designer>
	
	<!-- WindowGameWorld -->
	<designer name='WindowGameWorld' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!-- WindowLoading -->
	<designer name='WindowLoading' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!-- WindowConsole -->
	<designer name='WindowConsole' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.8'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
	</designer>
	
	<designer name='TextField.ConsoleInput' type='TextField'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
	</designer>
	
	<!-- ShoutConsole -->
	<designer name='ShoutConsole' type='Widget'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='31'>fonts/subtitle.defont</font>
	</designer>
	
	<!--
	Player choice widgets for conversation. These designers can be defined:
	- 'WindowPlayerChoice' -> WindowPlayerChoice
	- 'PlayerChoice' -> PanelPlayerChoice
	- 'PlayerChoiceEntry' -> PanelPlayerChoiceEntry
	- 'Label.PlayerChoiceEntry' -> PanelPlayerChoiceEntry Label
	- 'Image.PlayerChoiceEntry' -> PanelPlayerChoiceEntry DisplayImage
	-->
	<designer name='PlayerChoiceEntry' type='PlayerChoiceEntry'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<border parameter='normal.border' type='Line'>
			<color parameter='color' r='0' g='0' b='0' a='0.5'/>
			<integer parameter='thickness'>5</integer>
		</border>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0.5' a='0.5'/>
		</canvasCreator>
		<border parameter='hover.border' type='Line'>
			<color parameter='color' r='0' g='0' b='0.5' a='0.5'/>
			<integer parameter='thickness'>5</integer>
		</border>
	</designer>
	
	<designer name='Label.PlayerChoiceEntry' extendNamed='Label'>
		<color parameter='normal.textColor' r='0.6' g='0.6' b='0.6'/>
		<font parameter='normal.font' size='31'>fonts/subtitle.defont</font>
	</designer>
	
	<!--
	Sub-title widgets for conversation and other purpose. These designer can be defined:
	- 'WindowSubTitle' -> WindowSubTitle
	- 'SubTitleBox' -> SubTitleBox
	- 'SubTitleBoxText' -> SubTitleBoxText
	-->
	<designer name='SubTitleBoxText' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<border parameter='normal.border' type='Line'>
			<color parameter='color' r='0' g='0' b='1' a='0.5'/>
			<integer parameter='thickness'>2</integer>
		</border>
		<borderSize parameter='normal.padding' left='20' top='5' right='20' bottom='5'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText' type='Label'>
		<borderSize parameter='normal.padding' left='5' top='0' right='5' bottom='0'/>
		<font parameter='normal.font' size='31'>fonts/subtitle.defont</font>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText.Text' extendNamed='Label.SubTitleBoxText'>
		<borderSize parameter='normal.padding' left='20' top='0' right='20' bottom='0'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText.Text.Binding' extendNamed='Label.SubTitleBoxText'>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
	
	<!--
	Widgets for editing input bindings. These designers can be defined:
	- 'ListBox.EditBindings'
	    EditBindingsListBox
	- 'Panel.EditBindingsEntry' {+'.Selected' for selected}
	    EditBindingsListRenderer root panel
	- 'Label.EditBindingsEntry.Name' {+'.Selected' for selected}
	    EditBindingsListRenderer name label with command name to configurate bindings for
	- 'Label.EditBindingsEntry.Binding' {+'.Selected' for selected}
	    EditBindingsListRenderer binding label
	- 'WindowDialog.CaptureBinding'
	    Dialog window for capturing binding
	- 'Label.WindowDialog.CaptureBinding'
	    Text in capture binding dialog
	- 'Panel.WindowDialogButtons.CaptureBinding'
	    Button panel in capture binding dialog
	- 'Button.WindowDialog.CaptureBinding'
	    Button in capture binding dialog button panel
	-->
	<designer name='Panel.EditBindingsEntry' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Default'/>
		<borderSize parameter='normal.padding' left='5' top='2' right='5' bottom='2'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5' a='0.1'/>
		</canvasCreator>
	</designer>
	
	<designer name='Panel.EditBindingsEntry.Selected' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5'/>
		</canvasCreator>
		<borderSize parameter='normal.padding' left='5' top='2' right='5' bottom='2'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.3' g='0.3' b='0.5'/>
		</canvasCreator>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Name' extendNamed='Label'>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Name.Selected' extendNamed='Label'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Binding' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'>
			<colorMatrix parameter='colorMatrix'>
				<scale r='0.9' g='1' b='0.9'/>
			</colorMatrix>
		</canvasCreator>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'>
				<colorMatrix parameter='colorMatrix'>
					<scale r='0.6' g='0.6' b='0.6'/>
				</colorMatrix>
			</canvasCreator>
			<borderSize parameter='size' all='2'/>
		</border>
		<borderSize parameter='normal.padding' left='4' top='1' right='4' bottom='1'/>
	</designer>
	
	<designer name='Label.DisplayInputSource' type='Label'>
		<borderSize parameter='normal.padding' all='3'/>
		<font parameter='normal.font' size='26'>fonts/binding.defont</font>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
	
	<!--
	Widgets for editing system parameters. These designers can be defined:
	- 'Panel.EditModuleParametersPanel'
	    EditModuleParametersPanel
	- 'Button.EditModuleParametersPanel.System'
	    Button to switch to system panel
	- 'Panel.EMPSystem.Content'
	    EMPSystem
	- 'TextArea.EMPSystem.ParameterInfo'
	    TextArea in EMPSystem showing parameter information
	- 'Label.EMPSystem.Name'
	    Label in EMPSystem showing name of parameter
	- 'TextField.EMPSystem.Value'
	    TextField edit string value
	- 'CheckBox.EMPSystem.Value'
	    CheckBox edit boolean value
	- 'Panel.EMPSystem.CycleValue'
	    Cycle value panel
	- 'Button.EMPSystem.CycleValue.Prev'
	    Previous value button in cycle value panel
	- 'Button.EMPSystem.CycleValue.Next'
	    Next value button in cycle value panel
	- 'Label.EMPSystem.CycleValue.Value'
	    Value label in cycle value panel
	-->
	<designer name='Panel.EMPSystem.Content' type='Widget'>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' all='2'/>
		</border>
	</designer>
	
	<designer name='Panel.EMPSystem.Parameters' type='Widget'>
		<borderSize parameter='normal.padding' left='5' top='2' right='5' bottom='2'/>
	</designer>
	
	<designer name='Panel.EMPSystem.CycleValue' type='Widget'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'/>
		<border parameter='normal.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/normal.ccreator.xml'/>
			<borderSize parameter='size' left='0' top='2' right='0' bottom='2'/>
		</border>
		<borderSize parameter='normal.padding' left='2' right='2'/>
		
		<!--
		<canvasCreator parameter='disabled.canvas' extend='framed/background-disabled.ccreator.xml'/>
		<border parameter='disabled.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/disabled.ccreator.xml'/>
			<borderSize parameter='size' left='0' top='2' right='0' bottom='2'/>
		</border>
		-->
		
		<border parameter='hover.border' type='CanvasCreator'>
			<canvasCreator parameter='canvasCreator' extend='framed/hover.ccreator.xml'/>
			<borderSize parameter='size' left='0' top='2' right='0' bottom='2'/>
		</border>
	</designer>
	
	<designer name='Label.EMPSystem.CycleValue.Value' type='Label'>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>fonts/sans15.defont</font>
	</designer>
	
	
	<!--
	PanelInteractPrompt.
	- 'InteractPrompt': The prompt panel
	- 'InteractPromptEntry': The prompt entry panel
	- 'Image.InteractPromptEntry': Interact image
	- 'Video.InteractPromptEntry': Interact video
	- 'Label.InteractPromptEntry.Verb': Verb label
	- 'Label.InteractPromptEntry.Description': Description label
	- 'Panel.InteractPromptEntry.Bindings': Bindings panel
	-->
	<designer name='InteractPrompt' type='Widget'>
		<canvasCreator parameter='normal.canvas' extend='framed/background.ccreator.xml'>
			<colorMatrix parameter='colorMatrix'>
				<scale r='0' g='0' b='0' a='0.5'/>
			</colorMatrix>
		</canvasCreator>
		<borderSize parameter='normal.padding' all='10'/>
	</designer>
	
	<designer name='Label.InteractPromptEntry' type='Label'>
		<font parameter='normal.font' size='31'>../modern/fonts/subtitle.defont</font>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
	</designer>
	
	
	<!--
	Logo window. These designer can be defined:
	- WindowLogo: Window itself
	- Panel.WindowLogo: Aspect ratio panel containing logo
	- Video.WindowLogo: Video widget
	- Image.WindowLogo: Image widget
	-->
	<designer name='WindowLogo' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!--
	Interactive main menu. These designers can be define:
	- 'InteractiveMainMenu' -> InteractiveMainMenu
	-->
	<designer name='InteractiveMainMenu' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
	</designer>
	
	<designer name='Panel.IMMTextDisplay' type='Widget'>
		<borderSize parameter='normal.padding' all='50'/>
	</designer>
	
	<designer name='Label.IMMTextDisplay' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='30'>fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='20' top='3' right='20' bottom='3'/>
	</designer>
	
	<!--
	PanelDebugInformation: panel
	Label.PanelDebugInformation.Label: label
	Label.PanelDebugInformation.Details: details
	-->
	<designer name='PanelDebugInformation' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.75'/>
		</canvasCreator>
		<borderSize parameter='normal.padding' all='2'/>
	</designer>
	
	<designer name='Label.PanelDebugInformation' type='Label'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='13'>fonts/fixed9.defont</font>
		<string parameter='normal.alignment'>left,middle</string>
	</designer>
	
	<designer name='Label.PanelDebugInformation.Label' extendNamed='Label.PanelDebugInformation'>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
	
	<!-- Behavior ECBehaviorRenderablePanel -->
	<designer name='RenderablePanel' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='1' g='1' b='1'/>
		</canvasCreator>
	</designer>
	
	
	<!--
	WindowSettings:
	- "Window.Settings": Settings window
	- "Panel.Settings.Sheets": Panel containing all settings panels
	- "Panel.Settings.Buttons.Top": Top buttons panel
	- "Panel.Settings.Buttons.Bottom": Bottom buttons panel
	- "Panel.Settings.Buttons.Left": Left buttons panel
	- "Panel.Settings.Buttons.Right": Right buttons panel
	- "Panel.Settings.Buttons.Dialog": Dialog buttons panel at the bottom
	- "Button.Settings.Sheet": Button to activate a sheet
	- "Button.Settings.Save": Button to save and close window
	- "Button.Settings.Reset": Button to reset current sheet
	- "Button.Settings.Cancel": Button to discard changes and close window
	-->
	<designer name='Window.Settings' extendNamed='Window'>
		<borderSize parameter='normal.padding' all='20'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Top' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='0' right='0' bottom='10'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Bottom' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='10' right='0' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Left' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='0' right='10' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Right' type='Widget'>
		<borderSize parameter='normal.padding' left='10' top='0' right='0' bottom='0'/>
	</designer>
	<designer name='Panel.Settings.Buttons.Dialog' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='20' right='0' bottom='0'/>
	</designer>
	
	
	<!--
	PanelFPSRates.
	- "Panel.FPSRates": Entire panel
	- "Label.Panel.FPSRates.{Module}": Label for module
	- "Label.Panel.FPSRates.Rate.{Module}": Rate for module
	
	Where "{Module}" can be:
	- "Script": Scripting module (Engine itself)
	- "Graphic": Graphic module
	- "Audio": Audio module
	- "Physics": Physics module
	- "Network": Network module
	-->
	<designer name='Panel.FPSRates' type='Widget'>
