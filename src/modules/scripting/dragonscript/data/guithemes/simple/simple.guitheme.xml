<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE guiTheme SYSTEM 'DragengineGuiTheme.dtd'>
<guiTheme>
	<!-- Default widget parameters. -->
	<font for='widget' size='15'>../modern/fonts/sans15.defont</font>
	
	<!-- Widget designers -->
	<designer name='Desktop' type='Desktop'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Image'>
			<image parameter='image'>../modern/mousepointer/default.png</image>
		</mousePointer>
	</designer>
	
	<designer name='Window' type='Window'>
		<decoration parameter='decoration' type='Simple'>
			<font parameter='active.title.font' size='15'>../modern/fonts/sans15.defont</font>
			<color parameter='active.title.textColor' r='0' g='0' b='0'/>
			<color parameter='inactive.title.textColor' r='0.25' g='0.25' b='0.25'/>
		</decoration>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.85' g='0.82' b='0.82'/>
		</canvasCreator>
	</designer>
	
	<designer name='WindowPopup' type='Window'>
		<!--<decoration parameter='decoration' type='CanvasCreator'>
			<borderSize parameter='decorationSize' all='1'/>
			<canvasCreator parameter='active.canvas' type='Bevel'>
				<color parameter='colorHilight' r='1' g='1' b='1'/>
				<color parameter='colorShadow' r='0.6' g='0.6' b='0.6'/>
			</canvasCreator>
		</decoration>-->
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.85' g='0.82' b='0.82'/>
		</canvasCreator>
	</designer>
	
	<designer name='WindowFullScreen' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
	</designer>
	
	<designer name='Window.Dialog' extendNamed='Window'>
		<borderSize parameter='normal.padding' all='20'/>
	</designer>
	
	<designer name='Panel.WindowDialogButtons' type='Widget'>
		<borderSize parameter='normal.padding' left='0' top='20' right='0' bottom='0'/>
	</designer>
	
	<designer name='Label' type='Label'>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
		<color parameter='disabled.textColor' r='0.5' g='0.5' b='0.5'/>
	</designer>
	
	<designer name='Button' type='Button'>
		<canvasCreator parameter='normal.canvas' extend='normal.ccreator.xml'/>
		<canvasCreator parameter='pressed.canvas' extend='pressed.ccreator.xml'/>
		<canvasCreator parameter='disabled.canvas' extend='disabled.ccreator.xml'/>
		<canvasCreator parameter='hover.canvas' extend='hover.ccreator.xml'/>
		<borderSize parameter='normal.padding' all='5'/>
		<borderSize parameter='pressed.padding' left='6' top='6' right='4' bottom='4'/>
	</designer>
	
	<designer name='ScrollBar' type='ScrollBar'>
		<canvasCreator parameter='canvasFrame' type='Bevel'>
			<color parameter='colorHilight' r='0.5' g='0.5' b='0.5'/>
			<color parameter='colorShadow' r='1' g='1' b='1'/>
			<color parameter='colorBackground' r='0.86' g='0.86' b='0.86'/>
		</canvasCreator>
		<canvasCreator parameter='canvasKnob' type='Bevel'>
			<color parameter='colorHilight' r='1' g='1' b='1'/>
			<color parameter='colorShadow' r='0.5' g='0.5' b='0.5'/>
			<color parameter='colorBackground' r='0.86' g='0.86' b='0.86'/>
		</canvasCreator>
		<integer parameter='size'>16</integer>
		<designer parameter='buttonUp' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonDown' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonLeft' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonRight' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
	</designer>
	
	<designer name='SpinButton' type='SpinButton'>
		<integer parameter='size'>16</integer>
		<designer parameter='buttonUp' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
		<designer parameter='buttonDown' extend='button.wdesigner.xml'>
			<point parameter='normal.minimumSize' x='16' y='16'/>
		</designer>
	</designer>
	
	<designer name='TextField' type='TextField'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<canvasCreator parameter='disabled.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.7' g='0.7' b='0.7'/>
		</canvasCreator>
		<border parameter='normal.border' type='Bevel'>
			<boolean parameter='raised'>false</boolean>
			<color parameter='colorHilight' r='1' g='1' b='1'/>
			<color parameter='colorShadow' r='0.5' g='0.5' b='0.5'/>
		</border>
		<borderSize parameter='normal.padding' left='2'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	<designer name='TextArea' type='TextArea'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<canvasCreator parameter='disabled.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.7' g='0.7' b='0.7'/>
		</canvasCreator>
		<border parameter='normal.border' type='Bevel'>
			<boolean parameter='raised'>false</boolean>
			<color parameter='colorHilight' r='1' g='1' b='1'/>
			<color parameter='colorShadow' r='0.5' g='0.5' b='0.5'/>
		</border>
		<borderSize parameter='normal.padding' left='2'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	<designer name='ListBox' type='ListBox'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<canvasCreator parameter='disabled.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.7' g='0.7' b='0.7'/>
		</canvasCreator>
		<border parameter='normal.border' type='Bevel'>
			<boolean parameter='raised'>false</boolean>
			<color parameter='colorHilight' r='1' g='1' b='1'/>
			<color parameter='colorShadow' r='0.5' g='0.5' b='0.5'/>
		</border>
	</designer>
	
	<designer name='Label.ListRenderer' type='Label'>
		<canvasCreator parameter='normal.canvas' type='Default'/>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5' a='0.1'/>
		</canvasCreator>
	</designer>
	
	<designer name='Label.ListRenderer.Selected' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.3' g='0.3' b='0.5'/>
		</canvasCreator>
	</designer>
	
	<designer name='ComboBox' type='ComboBox'>
		<canvasCreator parameter='normal.canvas' extend='normal.ccreator.xml'/>
		<canvasCreator parameter='droppedDown.canvas' extend='pressed.ccreator.xml'/>
		<canvasCreator parameter='disabled.canvas' extend='disabled.ccreator.xml'/>
		<canvasCreator parameter='hover.canvas' extend='hover.ccreator.xml'/>
		<borderSize parameter='normal.padding' left='4'/>
	</designer>
	
	<designer name='ListBox.ComboBox' extendNamed='ListBox'>
	</designer>
	
	<designer name='TextField.ComboBoxEditor' type='TextField'>
		<!--<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='1' g='1' b='1'/>
		</canvasCreator>-->
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='2' top='0' right='2' bottom='0'/>
	</designer>
	
	<designer name='CheckBox' type='CheckBox'>
		<borderSize parameter='normal.padding' all='5'/>
		<image parameter='normal.imageBox'>../modern/checkbox/checkbox_normal.png</image>
		<image parameter='normal.imageCheck'>../modern/checkbox/checked_normal.png</image>
		<image parameter='focused.imageBox'>../modern/checkbox/checkbox_focused.png</image>
		<image parameter='hover.imageBox'>../modern/checkbox/checkbox_hover.png</image>
		<image parameter='disabled.imageCheck'>../modern/checkbox/checked_disabled.png</image>
	</designer>
	
	<designer name='OptionBox' type='OptionBox'>
		<borderSize parameter='normal.padding' all='5'/>
		<image parameter='normal.imageBox'>../modern/optionbox/optionbox_normal.png</image>
		<image parameter='normal.imageSelect'>../modern/optionbox/selected_normal.png</image>
		<image parameter='focused.imageBox'>../modern/optionbox/optionbox_focused.png</image>
		<image parameter='hover.imageBox'>../modern/optionbox/optionbox_hover.png</image>
		<image parameter='disabled.imageSelect'>../modern/optionbox/selected_disabled.png</image>
	</designer>
	
	<designer name='ProgressBar' type='ProgressBar'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='0.9' b='0.9'/>
		</canvasCreator>
		<border parameter='normal.border' type='Bevel'>
			<boolean parameter='raised'>false</boolean>
			<color parameter='colorHilight' r='1' g='1' b='1'/>
			<color parameter='colorShadow' r='0.5' g='0.5' b='0.5'/>
		</border>
		<borderSize parameter='normal.padding' left='8' top='4' right='8' bottom='4'/>
		<canvasCreator parameter='bar' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='1'/>
		</canvasCreator>
	</designer>
	
	<!-- WindowGameWorld -->
	<designer name='WindowGameWorld' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!-- WindowLoading -->
	<designer name='WindowLoading' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!-- WindowConsole --> <!-- Window.Console -->
	<!-- ConsoleInputField --> <!-- TextField.ConsoleInput -->
	
	<!-- ShoutConsole -->
	<designer name='ShoutConsole' type='Widget'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='31'>../modern/fonts/subtitle.defont</font>
	</designer>
	
	<!--
	Player choice widgets for conversation. These designers can be defined:
	- "Window.PlayerChoice" -> WindowPlayerChoice
	- "PlayerChoice" -> PanelPlayerChoice
	- "PlayerChoiceEntry" -> PanelPlayerChoiceEntry
	-->
	<designer name='PlayerChoiceEntry' type='PlayerChoiceEntry'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0.5' a='0.5'/>
		</canvasCreator>
		<border parameter='normal.border' type='Line'>
			<color parameter='color' r='0' g='0' b='0' a='0.5'/>
			<integer parameter='thickness'>5</integer>
		</border>
		<border parameter='hover.border' type='Line'>
			<color parameter='color' r='0' g='0' b='0.5' a='0.5'/>
			<integer parameter='thickness'>5</integer>
		</border>
		<color parameter='normal.textColor' r='0.6' g='0.6' b='0.6'/>
		<font parameter='normal.font' size='31'>../modern/fonts/subtitle.defont</font>
	</designer>
	
	<!--
	Sub-title widgets for conversation and other purpose. These designer can be defined:
	- "WindowSubTitle" -> WindowSubTitle
	- "SubTitleBox" -> SubTitleBox
	- "SubTitleBoxText" -> SubTitleBoxText
	-->
	<designer name='SubTitleBoxText' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<border parameter='normal.border' type='Line'>
			<color parameter='color' r='0' g='0' b='1' a='0.5'/>
			<integer parameter='thickness'>2</integer>
		</border>
		<borderSize parameter='normal.padding' left='20' top='5' right='20' bottom='5'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText' type='Label'>
		<borderSize parameter='normal.padding' left='5' top='0' right='5' bottom='0'/>
		<font parameter='normal.font' size='31'>../modern/fonts/subtitle.defont</font>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText.Text' extendNamed='Label.SubTitleBoxText'>
		<borderSize parameter='normal.padding' left='20' top='0' right='20' bottom='0'/>
	</designer>
	
	<designer name='Label.SubTitleBoxText.Text.Binding' extendNamed='Label.SubTitleBoxText'>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
	
	<!--
	Widgets for editing input bindings. These designers can be defined:
	- 'ListBox.EditBindings'
	    EditBindingsListBox
	- 'Panel.EditBindingsEntry' {+'.Selected' for selected}
	    EditBindingsListRenderer root panel
	- 'Label.EditBindingsEntry.Name' {+'.Selected' for selected}
	    EditBindingsListRenderer name label with command name to configurate bindings for
	- 'Label.EditBindingsEntry.Binding' {+'.Selected' for selected}
	    EditBindingsListRenderer binding label
	- 'Window.CaptureBinding'
	    Dialog window for capturing binding
	- 'Label.WindowDialog.CaptureBinding'
	    Text in capture binding dialog
	- 'Panel.WindowDialogButtons.CaptureBinding'
	    Button panel in capture binding dialog
	- 'Button.WindowDialog.CaptureBinding'
	    Button in capture binding dialog button panel
	-->
	<designer name='Panel.EditBindingsEntry' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Default'/>
		<borderSize parameter='normal.padding' left='5' top='2' right='5' bottom='2'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5' a='0.1'/>
		</canvasCreator>
	</designer>
	
	<designer name='Panel.EditBindingsEntry.Selected' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.2' g='0.2' b='0.5'/>
		</canvasCreator>
		<borderSize parameter='normal.padding' left='5' top='2' right='5' bottom='2'/>
		
		<canvasCreator parameter='hover.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.3' g='0.3' b='0.5'/>
		</canvasCreator>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Name' extendNamed='Label'>
		<color parameter='normal.textColor' r='0' g='0' b='0'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Name.Selected' extendNamed='Label'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='15'>../modern/fonts/sans15.defont</font>
	</designer>
	
	<designer name='Label.EditBindingsEntry.Binding' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0.9' g='1' b='0.9'/>
		</canvasCreator>
		<border parameter='normal.border' type='Line'>
			<color parameter='color' r='0.6' g='0.6' b='0.6'/>
		</border>
		<borderSize parameter='normal.padding' left='2' top='1' right='2' bottom='1'/>
	</designer>
	
	<designer name='Label.DisplayInputSource' type='Label'>
		<borderSize parameter='normal.padding' all='3'/>
		<font parameter='normal.font' size='26'>../modern/fonts/binding.defont</font>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
	
	<!--
	Widgets for editing system parameters. These designers can be defined:
	- 'Panel.EditModuleParametersPanel'
	    EditModuleParametersPanel
	- 'Button.EditModuleParametersPanel.System'
	    Button to switch to system panel
	- 'Panel.EMPSystem'
	    EMPSystem
	- 'TextArea.EMPSystem.ParameterInfo'
	    TextArea in EMPSystem showing parameter information
	- 'Label.EMPSystem.Name'
	    Label in EMPSystem showing name of parameter
	- 'TextField.EMPSystem.Value'
	    TextField edit string value
	- 'CheckBox.EMPSystem.Value'
	    CheckBox edit boolean value
	- 'Panel.EMPSystem.CycleValue'
	    Cycle value panel
	- 'Button.EMPSystem.CycleValue.Prev'
	    Previous value button in cycle value panel
	- 'Button.EMPSystem.CycleValue.Next'
	    Next value button in cycle value panel
	- 'Label.EMPSystem.CycleValue.Value'
	    Value label in cycle value panel
	-->
	
	<!--
	Logo window. These designer can be defined:
	- WindowLogo: Window itself
	- Panel.WindowLogo: Aspect ratio panel containing logo
	- Video.WindowLogo: Video widget
	- Image.WindowLogo: Image widget
	-->
	<designer name='WindowLogo' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
		<mousePointer parameter='normal.mousePointer' type='Default'/>
	</designer>
	
	<!--
	Interactive main menu. These designers can be define:
	- 'InteractiveMainMenu' -> InteractiveMainMenu
	-->
	<designer name='InteractiveMainMenu' type='Window'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0'/>
		</canvasCreator>
	</designer>
	
	<designer name='Panel.IMMTextDisplay' type='Widget'>
		<borderSize parameter='normal.padding' all='50'/>
	</designer>
	
	<designer name='Label.IMMTextDisplay' extendNamed='Label'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.5'/>
		</canvasCreator>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='30'>../modern/fonts/sans15.defont</font>
		<borderSize parameter='normal.padding' left='20' top='3' right='20' bottom='3'/>
	</designer>
	
	<!--
	PanelDebugInformation: panel
	Label.PanelDebugInformation.Label: label
	Label.PanelDebugInformation.Details: details
	-->
	<designer name='PanelDebugInformation' type='Widget'>
		<canvasCreator parameter='normal.canvas' type='Rectangle'>
			<color parameter='colorFill' r='0' g='0' b='0' a='0.75'/>
		</canvasCreator>
		<borderSize parameter='normal.padding' all='2'/>
	</designer>
	
	<designer name='Label.PanelDebugInformation' type='Label'>
		<color parameter='normal.textColor' r='1' g='1' b='1'/>
		<font parameter='normal.font' size='13'>../modern/fonts/fixed9.defont</font>
		<string parameter='normal.alignment'>left,middle</string>
	</designer>
	
	<designer name='Label.PanelDebugInformation.Label' extendNamed='Label.PanelDebugInformation'>
		<color parameter='normal.textColor' r='1' g='1' b='0'/>
	</designer>
</guiTheme>
